{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/titulos/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/bible-content/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/bible-files/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/check-consecutive/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/fix-letter-verses/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/fix-verses/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/format-book/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/format-comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/get-comment/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/get-parallels/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/get-parallels-list/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/link-notes/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/prepare-book/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/search/?$",
			"dest": "_render"
		},
		{
			"src": "^/buscar/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}